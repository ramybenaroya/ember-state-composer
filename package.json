{
  "name": "ember-state-composer",
  "version": "0.1.2",
  "description": "ember-state-composer makes it easier to programmatically define state on route controllers. Composing state makes it easy to group related functionality into logical objects that are easy to present in templates.",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test"
  },
  "repository": "https://github.com/taras/ember-state-composer.git",
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "author": "",
  "license": "MIT",
