    "broccoli-merge-trees": "^0.1.4",
    "grunt-cli": "^0.1.13",
    "grunt-broccoli": "^0.3.6",
    "grunt": "^0.4.5",
    "grunt-bump": "0.0.15",
    "grunt-shell": "^1.1.1",
    "broccoli-es6-concatenator": "^0.1.7",
    "broccoli-static-compiler": "^0.1.4",
    "ember-cli": "0.0.46",
    "ember-cli-ember-data": "0.1.0",
    "ember-cli-ic-ajax": "0.1.1",
